{
  "expo": {
    "name": "Mangerine",
    "slug": "mangerine",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#111D4A"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mangerine.app",
      "splash": {
      "image": "./assets/splash-screen.png",
        "resizeMode": "contain",
        "backgroundColor": "#111D4A"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#111D4A"
      },
      "splash": {
      "image": "./assets/splash-screen.png",
        "resizeMode": "contain",
        "backgroundColor": "#111D4A"
      },
      "package": "com.mangerine.app",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-dev-client",
      "expo-font",
      "expo-splash-screen"
    ],
    "scheme": "mangerine",
    "extra": {
      "eas": {
        "projectId": "39bbc663-3a09-427f-b29b-5febc8b452c9"
      }
    },
    "owner": "vant-app"
  }
}
